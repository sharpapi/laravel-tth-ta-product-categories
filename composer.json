{
    "name": "sharpapi/laravel-tth-ta-product-categories",
    "description": "AI Tours & Activities Product Categorization for Laravel powered by SharpAPI.com",
    "keywords": [
        "sharpapi",
        "ai-powered",
        "ai capabilities",
        "api",
        "ai api",
        "api integration",
        "artificial intelligence",
        "natural language processing",
        "restful api",
        "php",
        "laravel",
        "software development",
        "content analysis",
        "categorization",
        "travel",
        "tourism",
        "hospitality",
        "tours",
        "activities",
        "product management"
    ],
    "homepage": "https://github.com/sharpapi/laravel-tth-ta-product-categories",
    "license": "MIT",
    "authors": [
        {
            "name": "Dawid Makowski",
            "email": "contact@sharpapi.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.1",
        "ext-json": "*",
        "laravel/framework": "^10.48.29|^11.0|^12.0",
        "sharpapi/php-core": "^1.0"
    },
    "require-dev": {
        "laravel/pint": "^1.0"
    },
    "autoload": {
        "psr-4": {
            "SharpAPI\\TthTaProductCategories\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "SharpAPI\\TthTaProductCategories\\Tests\\": "tests/"
        }
    },
    "scripts": {
        "post-autoload-dump": "",
        "build": [],
        "start": [
            "Composer\\Config::disableProcessTimeout",
            "@composer run build"
        ],
        "analyse": "vendor/bin/phpstan analyse",
        "test": "vendor/bin/pest",
        "test-coverage": "vendor/bin/pest --coverage",
        "format": "vendor/bin/pint"
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true,
            "phpstan/extension-installer": true
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "SharpAPI\\TthTaProductCategories\\TthTaProductCategoriesProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
